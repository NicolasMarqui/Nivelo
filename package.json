    "@types/mongoose": "^5.10.3",
    "@types/node": "^14.14.22",
    "@types/nodemailer": "^6.4.0",
    "@types/redis": "^2.8.28",
    "@types/stripe": "^8.0.417",
    "@types/uuid": "^8.3.0",
    "nodemon": "^2.0.7"
  },
